{
    "Page": {
        "prefix": "tpage(waldo)",
        "body": [
            "page ${1:Id} \"${2:MyPage}\"",
            "{",
            "\tCaption = '${3:Caption}';",
            "\tPageType = ${4|Card,List,RoleCenter,CardPart,ListPart,Document,Worksheet,ListPlus,ConfirmationDialog,NavigatePage,StandardDialog,API,ReportPreview,ReportProcessingOnly,XmlPort|};",
            "\tApplicationArea = ${5|All,Basic,Suite,Advanced|};",
            "\tUsageCategory = ${6|Administration,Documents,History,Lists,None,ReportsAndAnalysis,Tasks|};",
            "\tSourceTable = ${7:TableName};",
            "\t",
            "\tlayout",
            "\t{",
            "\t\tarea(Content)",
            "\t\t{",
            "\t\t\tgroup(${8:GroupName})",
            "\t\t\t{",
            "\t\t\t\tfield(${8:MyField; FieldSource})",
            "\t\t\t\t{",
            "\t\t\t\t\tApplicationArea = ${5};",
            "\t\t\t\t}",
            "\t\t\t\t${0}",
            "\t\t\t}",
            "\t\t}",
            "\t}",
            "\t",
            "\tactions",
            "\t{",
            "\t\tarea(${9:Processing})",
            "\t\t{",
            "\t\t\taction(${10:ActionName})",
            "\t\t\t{",
            "\t\t\t\tApplicationArea = ${5};",
            "\t\t\t\t",
            "\t\t\t\ttrigger OnAction()",
            "\t\t\t\tbegin",
            "\t\t\t\t\t",
            "\t\t\t\tend;",
            "\t\t\t}",
            "\t\t}",
            "\t}",
            "}"
        ],
        "description": "Snippet: Page"
    },
    "Snippet: Page Field": {
        "prefix": "tpagefield(waldo)",
        "body": [
            "field(${1:MyField; FieldSource})",
            "{",
            "\tApplicationArea = ${3|All,Basic,Suite,Advanced|};",
            "}",
            "$0"
        ],
        "description": "Snippet: Page Field"
    },
    "Snippet: Action": {
        "prefix": "taction(waldo)",
        "body": [
            "action(${1:ActionName})",
            "{",
            "\tApplicationArea = ${2|All,Basic,Suite,Advanced|};",
            "\t",
            "\ttrigger OnAction()",
            "\tbegin",
            "\t\t$0",
            "\tend;",
            "}"
        ],
        "description": "Snippet: Page Action"
    },
    "Snippet: User Control": {
        "prefix": "tusercontrol(waldo)",
        "body": [
            "usercontrol(${1:ControlName}; ${2:AddInName})",
            "{",
            "\tApplicationArea = ${3|All,Basic,Suite,Advanced|};",
            "\t",
            "\ttrigger ${4:MyTrigger()}",
            "\tbegin",
            "\t\t$0",
            "\tend;",
            "}"
        ],
        "description": "Snippet: Page User Control"
    },
    "Snippet: View Shared Layout": {
        "prefix": "tviewshared(waldo)",
        "body": [
            "view(${1:ViewName})",
            "{",
            "\tCaption = '${2:ViewCaption}';",
            "\tFilters = where(${3:TableFieldName} = filter(${0:FilterValue}));",
            "}"
        ],
        "description": "Snippet: Page View without layout changes"
    },
    "Snippet: View Detached Layout": {
        "prefix": "tviewdetached(waldo)",
        "body": [
            "view(${1:ViewName})",
            "{",
            "\tCaption = '${2:ViewCaption}';",
            "\tFilters = where(${3:TableFieldName} = filter(${4:FilterValue}));",
            "\tSharedLayout = false;",
            "\t",
            "\tlayout",
            "\t{",
            "\t\t$0",
            "\t}",
            "}"
        ],
        "description": "Snippet: Page View with layout changes"
    },
    "Snippet: Part": {
        "prefix": "tpart(waldo)",
        "body": [
            "part(${1:PartName}; ${2:PartSource})",
            "{",
            "\t${0}",
            "}"
        ],
        "description": "Snippet: Page Part"
    },    
    "Page of type list": {
        "prefix": "tpagelist(waldo)",
        "body": [
            "page ${1:Id} ${2:PageName}",
            "{",
            "\tCaption = '${3:PageName}';",
            "\tPageType = List;",
            "\tUsageCategory = Lists;",
            "\tApplicationArea = ${4|All,Basic,Suite,Advanced|};",
            "\tSourceTable = ${5:TableName};",
            "\t",
            "\tlayout",
            "\t{",
            "\t\tarea(Content)",
            "\t\t{",
            "\t\t\trepeater(${6:Group})",
            "\t\t\t{",
            "\t\t\t\tfield(${7:MyField; FieldSource})",
            "\t\t\t\t{",
            "\t\t\t\t\tApplicationArea = ${4};",
            "\t\t\t\t}",
            "\t\t\t\t${9}",
            "\t\t\t}",
            "\t\t}",
            "\t\tarea(Factboxes)",
            "\t\t{",
            "\t\t\t${10}",
            "\t\t}",
            "\t}",
            "\t",
            "\tactions",
            "\t{",
            "\t\tarea(${11:Processing})",
            "\t\t{",
            "\t\t\taction(${12:ActionName})",
            "\t\t\t{",
            "\t\t\t\tApplicationArea = ${4};",
            "\t\t\t\t",
            "\t\t\t\ttrigger OnAction();",
            "\t\t\t\tbegin",
            "\t\t\t\t\t$0",
            "\t\t\t\tend;",
            "\t\t\t}",
            "\t\t}",
            "\t}",
            "}"
        ],
        "description": "Snippet: Page of type list"
    },
    "Page of type API": {
        "prefix": "tpageapi(waldo)",
        "body": [
            "page ${1:Id} ${2:ApiPageName}",
            "{",
            "\tPageType = API;",
            "\tCaption = '${3:apiPageName}';",
            "\tAPIPublisher = '${4:publisherName}';",
            "\tAPIGroup = '${5:groupName}';",
            "\tAPIVersion = '${6:VersionList}';",
            "\tEntityName = '${7:entityName}';",
            "\tEntitySetName = '${8:entitySetName}';",
            "\tSourceTable = ${9:TableName};",
            "\tDelayedInsert = true;",
            "\t",
            "\tlayout",
            "\t{",
            "\t\tarea(Content)",
            "\t\t{",
            "\t\t\trepeater(${10:GroupName})",
            "\t\t\t{",
            "\t\t\t\tfield(${11:fieldName; NameSource})",
            "\t\t\t\t{",
            "\t\t\t\t\tCaption = '${12:fieldCaption}';",
            "\t\t\t\t\t${0}",
            "\t\t\t\t}",
            "\t\t\t}",
            "\t\t}",
            "\t}",
            "}"
        ],
        "description": "Snippet: Page of type API"
    },
    "Page of type Role Center": {
        "prefix": "tpage(waldo)",
        "body": [
            "page ${1:Id} ${2:RoleCenterName}",
            "{",
            "\tPageType = RoleCenter;",
            "\t",
            "\tlayout",
            "\t{",
            "\t\tarea(RoleCenter)",
            "\t\t{",
            "\t\t\t$0",
            "\t\t}",
            "\t}",
            "\t",
            "\tactions",
            "\t{",
            "\t\tarea(Creation)",
            "\t\t{",
            "\t\t\taction(${3:ActionBarAction})",
            "\t\t\t{",
            "\t\t\t\tRunObject = ${4|Page,Report,XmlPort,Codeunit|} ${5:ObjectName};",
            "\t\t\t}",
            "\t\t}",
            "\t\tarea(Sections)",
            "\t\t{",
            "\t\t\tgroup(${6:SectionsGroupName})",
            "\t\t\t{",
            "\t\t\t\taction(${7:SectionsAction})",
            "\t\t\t\t{",
            "\t\t\t\t\tRunObject = ${8|Page,Report,XmlPort,Codeunit|} ${9:ObjectName};",
            "\t\t\t\t}",
            "\t\t\t}",
            "\t\t}",
            "\t\tarea(Embedding)",
            "\t\t{",
            "\t\t\taction(${10:EmbeddingAction})",
            "\t\t\t{",
            "\t\t\t\tRunObject = ${11|Page,Report,XmlPort,Codeunit|} ${12:ObjectName};",
            "\t\t\t}",
            "\t\t}",
            "\t}",
            "}"
        ],
        "description": "Snippet: Page of type Role Center"
    }
}